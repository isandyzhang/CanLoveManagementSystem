@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@await Component.InvokeAsync("SelectCase", new
{
    targetController = ViewBag.TargetController as string ?? "CaseOpening",
    targetAction = ViewBag.TargetAction as string ?? "Create",
    targetTab = ViewBag.TargetTab as string ?? "",
    targetStep = ViewBag.TargetStep as string ?? "",
    mode = ViewBag.Mode as string ?? "Create",
    autoLoad = ViewBag.AutoLoad as bool? ?? false
})

